#: import NoTransition kivy.uix.screenmanager.NoTransition
#: import Factory kivy.factory.Factory
<MyDropDown@DropDown>:
    max_height: 435

<MyPopup@Popup>:
    auto_dismiss: True
    size_hint: 0.6, 0.85
    pos_hint: {"x": 0.2, "top": 0.9}
    title: "Forgot Your Password"

    MDBoxLayout:
        orientation: "vertical"

        MDLabel:
            text: "Email"
            font_size: 20
            color: 1,1,1,1

        MDTextField: 
            id: Email
            hint_text: "Email"
            icon_right: "email"
            size_hint_x: None
            width: 750
            height: 25
            font_size: 20
            mode: "round"
            padding_x: 10

        MDLabel:
            text: "Username"
            font_size: 20
            color: 1,1,1,1

        MDTextField: 
            id: User
            hint_text: "Username"
            icon_right: "account"
            size_hint_x: None
            width: 750
            height: 25
            font_size: 20
            mode: "round"
            padding_x: 10

        Button:
            text: "Recieve Code"
            font_size: 20
            on_release: app.CheckDetails(root)

        MDLabel:
            text: "Code"
            font_size: 20
            color: 1,1,1,1

        MDTextField: 
            id: Code
            hint_text: "Type Code"
            #icon_right: "eye-off"
            size_hint_x: None
            width: 750
            height: 25
            font_size: 20
            mode: "round"
            padding_x: 10
            disabled: True
        
        Button:
            id: CodeButton
            text: "Check Code"
            font_size: 20
            disabled: True
            on_release: app.CheckCode(root)

        MDLabel:
            text: "Password"
            font_size: 20
            color: 1,1,1,1

        MDTextField: 
            id: Pwd
            hint_text: "New Password"
            icon_right: "eye-off"
            size_hint_x: None
            width: 750
            height: 25
            font_size: 20
            mode: "round"
            padding_x: 10
            disabled: True
        
        Button: 
            id: Submit
            text: "Submit"
            font_size: 20
            disabled: True
            on_release: app.ChangePassword(root)

        Button: 
            text: "Close"
            font_size: 20
            on_release: root.dismiss()


        MDLabel: 
            id: ErrorMessage
            text: ""
            font_size: 20
            color: 1,1,1,1
        


        




<InputAndButton>:
    spacing: 15
    
    MDTextField:
        id: Title 
        hint_text: "Title"
        height: 25
        width: 850
        size_hint_x: None
        mode: "round"
    
    MDRaisedButton:
        id: Submit
        text: "Submit"
        on_release: 
            root.addset()
            app.addvalues("CreateSelection")

    
